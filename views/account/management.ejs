<section class="account-management">
  <h1><%= title %></h1>

  <%- messages() %>

  <% if (errors) { %>
    <ul class="notice">
      <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } %>

  <p>You're logged in.</p>

  <ul>
    <li><a href="/account/update/<%= accountData.account_id %>">Update Account Information</a></li>
  </ul>
</section>
